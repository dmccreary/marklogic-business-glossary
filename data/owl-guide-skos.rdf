    <dc:title>OWL Web Ontology Language Guide</dc:title>
    <doc:version>http://www.w3.org/TR/2004/REC-owl-guide-20040210/</doc:version>
    <rec:editor rdf:parseType="Resource">
      <contact:lastName>Smith</contact:lastName>
